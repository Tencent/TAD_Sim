{
  "name": "map-editor",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "dev:cloud": "vite --mode dev-cloud",
    "build": "vite build",
    "build-develop:cloud": "vite build --mode dev-cloud",
    "build-release:cloud": "vite build --mode prod-cloud",
    "build:cloud": "vite build --mode prod-cloud",
    "build:check": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "axios": "^1.5.1",
    "crypto-js": "^4.2.0",
    "deep-object-diff": "^1.1.7",
    "element-plus": "^2.6.1",
    "eventemitter3": "^5.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mousetrap": "^1.6.5",
    "number-precision": "^1.5.2",
    "pinia": "^2.1.7",
    "pixi.js": "7.2.4",
    "three": "^0.144.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.7.1",
    "workerpool": "^6.4.0"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.0",
    "@types/mousetrap": "^1.6.11",
    "@types/node": "^16.11.56",
    "@types/three": "^0.148.1",
    "@types/uuid": "^8.3.4",
    "@types/workerpool": "^6.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^20.0.0",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.2",
    "vite": "^4.5.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^0.23.0",
    "vue-tsc": "^1.8.5"
  }
}
